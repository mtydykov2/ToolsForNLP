# Unigram
U01:%x[-2,1]
U02:%x[-1,1]
U03:%x[0,0]
U04:%x[0,1]
U05:%x[0,2]
U06:%x[0,3]
U07:%x[0,4]
U08:%x[0,5]
U09:%x[0,6]
U10:%x[0,7]
U11:%x[0,8]
U12:%x[1,1]
U13:%x[2,1]

#bigram POS sequence
U14:%x[-2,1]/%x[-1,1]
U15:%x[-1,1]/%x[0,1]
U16:%x[0,1]/%x[1,1]
U17:%x[1,1]/%x[2,1]

#trigram POS sequence
U18:%x[-2,1]/%x[-1,1]/%x[0,1]
U19:%x[-1,1]/%x[0,1]/%x[1,1]
U20:%x[0,1]/%x[1,1]/%x[2,1]

