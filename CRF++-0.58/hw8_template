# Unigram
U01:%x[-2,1]
U02:%x[-1,1]
U03:%x[-2,2]
U04:%x[-1,2]
U05:%x[0,0]
U05:%x[0,1]
U07:%x[0,2]
U08:%x[0,3]
U09:%x[0,4]
U10:%x[0,5]
U11:%x[0,6]
U12:%x[0,7]
U13:%x[1,1]
U14:%x[2,1]
U15:%x[1,2]
U16:%x[2,2]

#bigram POS sequence
U17:%x[-2,1]/%x[-1,1]
U18:%x[-1,1]/%x[0,1]
U19:%x[0,1]/%x[1,1]
U20:%x[1,1]/%x[2,1]

#trigram POS sequence
U21:%x[-2,1]/%x[-1,1]/%x[0,1]
U22:%x[-1,1]/%x[0,1]/%x[1,1]
U23:%x[0,1]/%x[1,1]/%x[2,1]

